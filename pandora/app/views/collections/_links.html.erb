<% if @collection.links.blank? -%>
  <%= 'none'.t %>
<% else -%>
  <table>
    <% @collection.links.each_with_index do |link, index| -%>
    <tr>
      <td style="text-align: right; padding-right: 5px">[<%= index + 1 %>]</td>
      <td><%= link_to h(link), link %></td>
    </tr>
    <% end -%>
  </table>
<% end -%>

